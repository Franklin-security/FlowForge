# Example Git Configuration
# Add this to your ~/.gitconfig or run:
# git config --global user.name "Your Name"
# git config --global user.email "your.email@example.com"

[user]
    name = Your Name
    email = your.email@example.com

[core]
    autocrlf = input
    
[commit]
    template = .gitmessage

[init]
    defaultBranch = main

